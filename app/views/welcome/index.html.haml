.graph{:style => 'float:left;'}
  #cdf_div{:style => 'width:650px;height:300px;'}
    &nbsp;
.graph{:style => 'float:right;'}
  #k_graph_div{:style => 'width:650px;height:300px;'}
    &nbsp;


%pre
  = @k_graph_data.to_yaml


- @algorithms.each_with_index do |(algorithm_name, algorithm), index|
  %div{:class => (if index.even? then 'left' else 'right' end)}
    %div
      %strong= algorithm_name.to_s + '.'
    %div
      total found:
      = algorithm.map[0].length
    %div
      error:
      average -
      = algorithm.average_error
      max -
      = algorithm.max_error
    %div.graph
      - chart_id = algorithm_name.to_s + '_map'

      %div.map{:id => chart_id}
        &nbsp;
  - if index.odd?
    %div.clear

  %script
    algorithms['#{algorithm_name.to_s}'] = {cdf: #{algorithm.cdf}, map: #{algorithm.map}}



- unless @k_graph_data.nil?
  %script
    k_graph_data = #{@k_graph_data}





